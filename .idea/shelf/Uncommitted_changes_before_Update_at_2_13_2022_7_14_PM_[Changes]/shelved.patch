reventDefault();\r\n      }\r\n   };\r\n\r\n   const getData = async () => {\r\n      try {\r\n         const res = await clientApi.getClient(id)\r\n         setData(res.data)\r\n         setNumber(res.data.number)\r\n         setFullName(res.data.full_name)\r\n         setLoading(true)\r\n      } catch (err) {}\r\n   }\r\n\r\n   useEffect(() => {\r\n      const getData = async () => {\r\n         try {\r\n            const res = await clientApi.getClient(id)\r\n            setData(res.data)\r\n            setNumber(res.data.number)\r\n            setFullName(res.data.full_name)\r\n            setLoading(true)\r\n         } catch (err) {\r\n            console.log(err);\r\n         }\r\n      }\r\n      getData()\r\n   }, [id])\r\n\r\n   const deleteClientHandler = async (id) => {\r\n      try {\r\n         await clientApi.deleteClient(id)\r\n         toast.success('Mijoz o`chirildi')\r\n         navigate('/home/  clients')\r\n      } catch (err) {}\r\n   }\r\n\r\n   const updateClientHandler = async (e) => {\r\n      e.preventDefault()\r\n\r\n      const check = {\r\n         number: number.trim().length === 0\r\n      }\r\n\r\n      if(check.number){\r\n         toast.warning('Number kiritish majburiy')\r\n         return\r\n      }\r\n\r\n      let params = {}\r\n\r\n      fullName.length === 0 ? params = { number } : params = { number, full_name: fullName }\r\n\r\n      try {\r\n         await clientApi.upateClient(id, params)\r\n         toast.success('Mijoz yangilandi')\r\n         getData()\r\n\r\n         setTimeout(() => {\r\n            window.location.reload()\r\n         }, 2000)\r\n      } catch(err) {\r\n         toast.warning(err.response.data.number[0])\r\n         setNumber('')\r\n         setFullName('')\r\n         console.log(err.response);\r\n      }\r\n   }\r\n   return (\r\n      <div className='panel p-4'>\r\n         <Navbar/>\r\n         {loading ? (\r\n            <div className='row'>\r\n               <div className='col-12'>\r\n        