n    },\r\n    [setCurrentPage]\r\n  );\r\n  const currentData = data.slice(\r\n    (currentPage - 1) * LIMIT,\r\n    (currentPage - 1) * LIMIT + LIMIT\r\n  );\r\n\r\n  return (\r\n    <div className=\"panel p-4\">\r\n      <Navbar />\r\n      <form className=\"row\">\r\n        <div className=\"col-4\" >\r\n            <select className=\"form-select\" value={warehouse} onChange={e => setWarehouse(e.target.value)}>\r\n              <option value=''>Omborxonani tanlang</option>\r\n              {warehouses.map((item, index) => (\r\n              <option key={index} value={item.id}>\r\n                  {item.name}\r\n              </option>\r\n              ))}\r\n            </select>\r\n        </div>\r\n        <div className=\"col-4\">\r\n            <button className=\"btn btn-info text-white fw-bold mt-0\" onClick={handleClick}>\r\n              <i className=\"bi bi-check-circle del\"></i> Davom etish\r\n            </button>\r\n        </div>\r\n      </form>\r\n      <form className=\"form-row row mt-3\" onSubmit={updatedHandler}>\r\n        <div className=\"col-4\">\r\n            <label>Client ID</label>\r\n            <input\r\n              type=\"number\"\r\n              className=\"form-control w-100 p-2\"\r\n              placeholder=\"Client IDni kiriting\"\r\n              onKeyDown={handleEnter}\r\n              value={client}\r\n              onChange={(e) => setClient(e.target.value)}\r\n              ref={firstInput}\r\n            />\r\n        </div>\r\n        <div className=\"col-4\">\r\n            <label >Shtrix kodi</label>\r\n            <input\r\n              type=\"text\"\r\n              className=\"form-control w-100 p-2\"\r\n              placeholder=\"Shtrix kodi\"\r\n              onKeyDown={handleEnter}\r\n              value={barcode}\r\n              onChange={(e) => setBarcode(e.target.value)}\r\n            />\r\n        </div>\r\n        <div className=\"col-3\">\r\n            <label >Og'irligi</label>\r\n            <input\r\n              type=\"number\"\r\n              className=\"form-control w-100 p-2\"\r\n              placeholder=\"Og'irligi\"\r\n              value={weight}\r\n              onChange={(e) => setWeight(e.target.value)}\r\n            />\r\n        </div>\r\n        <div className=\"col-1\">\r\n            <button className=\"btn btn-success mt-4\">\r\n              <i className=\"bi bi-check-circle del p-2 \"></i>\r\n            </button>\r\n        </div>\r\n      </form>\r\n      <hr />\r\n      <div className=\"row mt-4 d-flex align-items-center justify-content-center\">\r\n        {/* NUM_OF_RECORDS={NUM_OF_RECORDS} */}\r\n        {loading ? <ReceiveList deleteOrderHandler={deleteOrderHandler} currentData={currentData} data={data} handleEnter={handleEnter} onPageChanged={onPageChanged} currentPage={currentPage} /> : <Loader />}\r\n      </div>\r\n\r\n      <ToastContainer />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Recive;","import {useState} from 'react';\r\nimport { flightApi } from '../../api/flightApi';\r\n\r\nconst FlightList = ({ item, index, handleEnter, toast, getFlights, data, deleteFlightHandler, role }) => {\r\n   const [name, setName] = useState();\r\n   const [fromAirport, setFromAirport] = useState('');\r\n   const [toAirport, setToAirport] = useState('');\r\n   \r\n   const updateHandler = async (e, id) => {\r\n      e.preventDefault();\r\n      \r\n      const check = {\r\n         name: name.trim().length === 0,\r\n         fromAirport: fromAirport.length === 0,\r\n         toAirport: toAirport.length === 0\r\n      }\r\n      \r\n      if(check.name || check.fromAirport || check.toAirport) {\r\n         toast.warning('Maydon to\\'ldirilmagan');\r\n         return;\r\n      }\r\n      \r\n      const params = {\r\n         name,\r\n         from_airport: fromAirport,\r\n         to_airport: toAirport\r\n      }\r\n      \r\n      try {\r\n         await flightApi.update(id, params);\r\n         getFlights();\r\n         toast.success('Airoport yangilandi');\r\n         setName('');\r\n         setToAirport('');\r\n         setFromAirport('');\r\n         setTimeout(() => {\r\n            window.location.reload();\r\n         }, 1500);\r\n      } catch (err) {}\r\n   }\r\n\r\n   return (\r\n      <tr className='text-center'>\r\n         <td>{index}</td>\r\n         <td>{item.id}</td>\r\n         <td>{item.name}</td>\r\n         {role === '100' ? (\r\n            <>\r\n               <td className='text-center'>\r\n                  <button className='btn btn-warning fw-bold text-white' data-bs-toggle=\"modal\" data-bs-target={`#update${index}`} onClick={() => {\r\n                     setName(item.name)\r\n                     setFromAirport(item.from_airport)\r\n                     setToAirport(item.to_airport)\r\n                  }}>\r\n                     <i className='bi bi-pencil'></i>\r\n                  </button>\r\n                  <div className=\"modal fade\" id={`update${index}`} aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n                     <div className=\"modal-dialog\">\r\n                        <div className=\"modal-content\">\r\n                           <div className=\"modal-header\">\r\n                              <h5 className=\"modal-title\" id=\"exampleModalLabel\">Tahrirlash</h5>\r\n                              <button type=\"button\" className=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n                           </div>\r\n                           <div className=\"modal-body\">\r\n                              <form onSubmit={(e) => {\r\n                                 updateHandler(e, item.id)\r\n                              }}>\r\n                                 <div className='mb-3'>\r\n                                    <input className='form-control' type='text' id='name' placeholder='name' onKeyDown={handleEnter} \r\n                                    value={name}\r\n                          